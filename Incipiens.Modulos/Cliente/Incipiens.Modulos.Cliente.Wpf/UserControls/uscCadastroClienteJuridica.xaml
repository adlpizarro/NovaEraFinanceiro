<UserControl x:Class="Incipiens.Modulos.Cliente.Wpf.UserControls.uscCadastroClienteJuridica"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Incipiens.Modulos.Pessoa.Wpf.UserControls;assembly=Incipiens.Modulos.Pessoa.Wpf"
             xmlns:uscPessoa ="clr-namespace:Incipiens.Modulos.Pessoa.Wpf.UserControls;assembly=Incipiens.Modulos.Pessoa.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="800">
    <Grid>
        <uscPessoa:uscCadastroPessoaJuridica x:Name="uscPessoaJuridica" DataContext="{Binding Path=_DadosCliente , UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" Validation.ErrorTemplate="{x:Null}"/>
    </Grid>
</UserControl>
