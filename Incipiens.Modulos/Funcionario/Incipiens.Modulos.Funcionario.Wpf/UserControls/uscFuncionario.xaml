<UserControl x:Class="Incipiens.Modulos.Funcionario.Wpf.UserControls.uscFuncionario"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Incipiens.Modulos.Funcionario.Wpf.UserControls"
             mc:Ignorable="d" 
             xmlns:uscPessoa ="clr-namespace:Incipiens.Modulos.Pessoa.Wpf.UserControls;assembly=Incipiens.Modulos.Pessoa.Wpf"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <uscPessoa:uscCadastroPessoaFisica x:Name="uscPessoaFisica" DataContext="{Binding Path=_DadosFuncionario, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Validation.ErrorTemplate="{x:Null}"/>
    </Grid>
</UserControl>
